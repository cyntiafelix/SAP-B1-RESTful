<?xml version="1.0" encoding="utf-8"?>	 
<configuration>
  <system.webServer>
    <handlers>
        <remove name="FlaskHandler" />	
      <add name="Python FastCGI"
           path="*"
           verb="*"
           modules="FastCgiModule"
           scriptProcessor="C:\Python27\python.exe|C:\Python27\Lib\site-packages\wfastcgi.py"
           resourceType="Unspecified"
           requireAccess="Script" />
    </handlers>
  </system.webServer>

  <appSettings>
    <!-- Required settings -->
    <add key="WSGI_HANDLER" value="app.app" />
    <add key="PYTHONPATH" value="C:\Users\Ventas\Desktop\drobles\SAP-B1-RESTful\flask" />

	 <!-- Optional settings -->
    <add key="WSGI_LOG" value="C:\Users\Ventas\Desktop\drobles\SAP-B1-RESTful\flask\logs\api.log" />
    <add key="WSGI_RESTART_FILE_REGEX" value=".*((\.py)|(\.config))$" />	 

	</appSettings>
</configuration>